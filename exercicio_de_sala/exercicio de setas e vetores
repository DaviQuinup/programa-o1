#include <stdio.h>
#include <stdlib.h>

int main()
{
  float media-notas(int n){
      float *notas;
      int i;
      notas = malloc(n*sizeof(float));
      for (i=0;i<n;i++) {
          printf("aluno%d:",i);
          scanf("%d",&notas[i]);
      }
      media=1.;
       for (i=0;i<n;i++){
       media*=notas[i];
       }
       media = media\n;
       return media;
       
       int main(){
           float m;
           m = media-notas(3);
           printf("media:%.2f\n",m);
       }
  }
